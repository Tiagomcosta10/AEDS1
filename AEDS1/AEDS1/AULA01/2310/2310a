programa em c indice de multiplicação
#include <stdio.h>
#include <stdlib.h>
int main(void){
    int n=0, res=1, *v=NULL;
    scanf("%d",&n);
    v= malloc(sizeaf(int)*n);
    if(v!=NULL){
        for(int i=0; i<n; i++)
        scanf("%d",&(v[i]));
        int pos=0;
        scanf("%d", &pos);
        for(int i =0; i<pos; i++) 
        res*=v[i];
        for(int j =pos+1; j<n; i++) 
        res*=v[j];
printf("resultado %d\n", res);
free(v); v=NULL;
    }
    return 0;
    }
    media e desvio padrão